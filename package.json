{
    "name": "AlloyDist",
    "version": "0.0.1",
    "description": "Alloy Distribute System for Web Project @ Tencent AlloyTeam",
    "keywords": [
        "Node.js",
        "mongodb",
        "Express"
    ],
    "main": "app.js",
    "private": true,
    "engines": {
        "node": ">=0.8.10"
